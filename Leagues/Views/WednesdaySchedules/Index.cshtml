@model IEnumerable<Leagues.Models.WednesdaySchedule>

@{
    ViewBag.Title = "Index";
}

<h2>Wednesday Schedule</h2>

<p>
    @Html.ActionLink("Add New Date", "Create")
</p>
<table class="table-condensed table-striped">
<tr>
    <th>
        @Html.DisplayNameFor(model => model.id)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.GameDate)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.IsCancelled)
    </th>
    <th></th>
</tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GameDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsCancelled)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.id })
        </td>
    </tr>
}

</table>
